{
  "name": "mathei-server",
  "version": "0.0.1",
  "description": "Node + Express API app generated by generator-wilbur",
  "main": "index.js",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register -r dotenv/config src/app.ts",
    "dev": "node build-scripts/build.js && nodemon --inspect=0.0.0.0:9229 --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node-dev' -r tsconfig-paths/register  src/app.ts",
    "build": "node build-scripts/build.js",
    "start:server": "node build-scripts/build.js && ts-node -r tsconfig-paths/register -r dotenv/config src/app.ts",
    "test": "node build-scripts/build.js && mocha test/**/*.ts --exit",
    "test:watch": "node build-scripts/build.js && mocha -r ts-node/register mocha test/**/*.ts  --watch --watch-extensions ts",
    "test:report": "node build-scripts/build.js && mocha test/**/*.ts --reporter mochawesome --exit",
    "test:coverage": "nyc mocha test/**/*.ts --exit --require ts-node/register --require source-map-support/register --recursive"
  },
  "dependencies": {
    "@typegoose/typegoose": "^6.4.0",
    "apollo-server": "^2.14.2",
    "apollo-server-express": "^2.11.0",
