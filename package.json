    "body-parser": "^1.19.0",
    "class-validator": "^0.11.1",
    "cookie-parser": "^1.4.5",
    "cookie-session": "^2.0.0-beta.3",
    "cors": "2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "5.3.1",
    "express-validator": "^6.4.0",
    "graphql": "^14.6.0",
    "graphql-subscriptions": "^1.1.0",
    "inversify": "^5.0.1",
    "inversify-express-utils": "^6.3.2",
    "istanbul": "^0.4.5",
    "js-yaml": "^3.13.1",
    "json-refs": "^3.0.15",
    "json2typescript": "^1.2.5",
    "jsonwebtoken": "8.5.1",
    "lodash": "^4.17.15",
    "mongoose": "^5.9.5",
    "nock": "^12.0.3",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "passport-google-token": "^0.1.2",
    "passport-local": "^1.0.0",
